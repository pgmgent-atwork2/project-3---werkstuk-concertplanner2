<section class="container register">
  <h4>Register</h4>
  {{>form-errors this}}
  <form method="post" action="/register">
    {{#each inputs}}
      {{>form-input this}}
    {{/each}}
    <div class="footer">
      <input type="reset" class="btn btn-tertiary" value="Clear">
      <input type="submit" class="btn btn-primary" value="Register">
    </div>
  </form>
</section>